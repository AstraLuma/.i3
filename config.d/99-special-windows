# Special window settings
# Chrome's app list: Default to float (esp since it poofs when it looses focus)
for_window [instance="chrome_app_list"] floating enable
# New google hangouts: Sizes wrong, but at least it's not taking up space.
for_window [instance="crx_knipolnnllmklapflnccelgolnpehhpl"] floating enable
# Journey, just want it floating
for_window [instance="crx_jlncjaehedpdoinepaejmlpbmdkgmpog"] floating enable
# galculator, same
for_window [instance="galculator"] floating enable

# Force starmade to not float
for_window [instance="(?i)^starmade"] floating disable
for_window[instance="starmade launcher"] floating enable
